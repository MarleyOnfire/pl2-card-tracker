{
  "rulesDirectory": ["tslint-plugin-prettier"],
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "rules": {
    "prettier": true,
    "no-empty": false,
    "no-empty-interface": false,
    "member-ordering": false,
    "no-console": false,
    "no-submodule-imports": false,
    "max-classes-per-file": false
  }
}
